<% provide(:title, 'create a interview') %>
<h1>create a interview</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for [:user, @interview] do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :日時 %>
      <%= f.text_field :time, class: 'form-control' %>

      <%= f.label :状態 %>
      <%= f.text_field :status, class: 'form-control' %>

      <%= f.submit "面接作成", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
